{
  "name": "portfolio",
  "version": "0.1.0",
  "main": "bundle.js",
  "dependencies": {
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "5.0.1",
    "style-loader": "^0.23.0"
  },
  "scripts": {
    "compile:sass": "node-sass src/sass/main.scss dist/css/styles.css",
    "build": "webpack --mode production",
    "dev-server": "webpack-dev-server --open --hot --mode development",
    "watch:css": "onchange 'src/sass/*/*.scss' -- npm run compile:sass",
    "start": "npm-run-all --parallel watch:css dev-server build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "file-loader": "^4.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "sass-loader": "^7.3.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0"
  }
}
